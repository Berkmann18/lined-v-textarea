{
  "name": "lined-vtextarea",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:md": "remark . .github -i ./.remarkignore",
    "lint:all": "npm run lint && npm run lint:md",
    "changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s -r 0",
    "version": "npm run changelog && git add CHANGELOG.md",
    "deploy": "gh-pages -d dist/"
  },
  "dependencies": {
    "sass-loader": "^7.1.0",
    "vue": "^2.5.17",
    "vuetify": "^2.1.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "cz-adapter-eslint": "^0.1.2",
    "eslint": "^5.8.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "gh-pages": "^2.0.1",
    "lint-staged": "^8.0.4",
    "node-sass": "^4.10.0",
    "remark-cli": "^6.0.0",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.5",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.0.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.md": [
      "remark",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 7.0.0",
    "npm": ">= 4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-adapter-eslint"
    }
  },
  "license": "MIT"
}
